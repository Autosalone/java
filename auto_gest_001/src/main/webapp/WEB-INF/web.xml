<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >
 

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>FiltroServlet</servlet-name>
  	<display-name>FiltroServlet</display-name>
  	<description></description>
  	<servlet-class>com.auto_gest_001.controller.FiltroServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>AmministratoreServlet</servlet-name>
  	<display-name>AmministratoreServlet</display-name>
  	<description></description>
  	<servlet-class>com.auto_gest_001.controller.AmministratoreServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>RegistrazioneServlet</servlet-name>
  	<display-name>RegistrazioneServlet</display-name>
  	<description></description>
  	<servlet-class>com.auto_gest_001.controller.RegistrazioneServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>LoginServlet</servlet-name>
  	<display-name>LoginServlet</display-name>
  	<description></description>
  	<servlet-class>com.auto_gest_001.controller.LoginServlet</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>FiltroServlet</servlet-name>
  	<url-pattern>/FiltroServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>AmministratoreServlet</servlet-name>
  	<url-pattern>/AmministratoreServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>RegistrazioneServlet</servlet-name>
  	<url-pattern>/RegistrazioneServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>LoginServlet</servlet-name>
  	<url-pattern>/LoginServlet</url-pattern>
  </servlet-mapping>
  
  
     <resource-ref>
    <description>DB Connection</description>
    <res-ref-name>jdbc/autosalone</res-ref-name>
    <res-type>javax.sql.DataSource</res-type>
    <res-auth>Container</res-auth>
  </resource-ref>
  
  
  
</web-app>
